{
  "name": "tysite-prpl",
  "version": "0.0.1",
  "description": "Rewrite of https://tyhopp.com using the PRPL static site generator",
  "main": "index.js",
  "scripts": {
    "fetch-remote-content": "node scripts/fetch-remote-content.js",
    "prestart": "prpl-build && node scripts/highlight-syntax.js",
